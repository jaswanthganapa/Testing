<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Segment__c</fullName>
    <description>The field specifies whether the Account belongs to the corporate or enterprise segment.</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(EmployeesRange__c , &quot;10.000+&quot;)|| ISPICKVAL(EmployeesRange__c , &quot;5001-10.000&quot;), &quot;Enterprise&quot;, &quot;Corporate&quot;)</formula>
    <label>Account Segment</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
