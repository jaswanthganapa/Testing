<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Potential_ARR_Score__c</fullName>
    <description>This field stores the score assigned to the account based on the Account Fit Score. It is utilized for customer segmentation assignment purposes.</description>
    <externalId>false</externalId>
    <formula>if(
     X2024_Fit_Score__c  = &quot;C&quot;, 0,
    if(
        X2024_Fit_Score__c = &quot;B&quot;, 20,
        40
    )
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Potential ARR (Score)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
