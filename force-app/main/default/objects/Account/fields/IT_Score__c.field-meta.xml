<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IT_Score__c</fullName>
    <description>based on HG_IT_Budget__c</description>
    <externalId>false</externalId>
    <formula>if( HG_IT_Budget__c &lt;1000000,0,
if( HG_IT_Budget__c &lt;5000000,25,
if( HG_IT_Budget__c &lt;10000000,50,
if( HG_IT_Budget__c &lt;25000000,75,100))))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>IT Budget Score</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
